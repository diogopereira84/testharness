<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="in.store.hide"
                   before="before.body.end" template="Fedex_Catalog::product/edit/instorehide.phtml" />
            <block class="Magento\Framework\View\Element\Template" name="catalog-selection"
                   before="before.body.end" template="Fedex_Catalog::product/catalog-selection.phtml">
                <arguments>
                    <argument name="view_model" xsi:type="object">Fedex\Catalog\ViewModel\CategoryCatalog</argument>
                </arguments>     
            </block>
            <block class="Fedex\Catalog\Block\Adminhtml\Product" name="commercial.shared.required"
                   before="before.body.end" template="Fedex_Catalog::product/commercial-shared.phtml" >
                   <arguments>
                        <argument name="view_model" xsi:type="object">Fedex\Catalog\ViewModel\CategoryCatalog</argument>
                    </arguments>    
            </block>
        </referenceContainer>
    </body>
</page>
