<?xml version="1.0"?>
<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <referenceBlock name="render.product.prices">
        <arguments>
            <argument name="default" xsi:type="array">
                <item name="prices" xsi:type="array">
                    <item name="unit_cost" xsi:type="array">
                        <item name="render_class" xsi:type="string">Fedex\Catalog\Pricing\Render\UnitCost</item>
                        <item name="render_template" xsi:type="string">Fedex_Catalog::product/price/unit_cost.phtml</item>
                    </item>
                </item>
            </argument>
        </arguments>
    </referenceBlock>
</layout>
