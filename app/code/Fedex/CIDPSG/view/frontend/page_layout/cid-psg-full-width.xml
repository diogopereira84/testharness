<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <update handle="1column" />
    <referenceContainer name="content">
        <block class="Magento\Framework\View\Element\Template" name="cid-psg-script" template="Fedex_CIDPSG::cid_psg_script.phtml" />
    </referenceContainer>
    <referenceContainer name="page.wrapper" htmlClass="page-wrapper page-layout-1column psg-cid-template-full-width layout-custom-column"/>
    <referenceContainer name="breadcrumbs" remove="true" />
</layout>
