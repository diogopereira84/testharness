<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Fedex\CartGraphQl\Model\Note\Command\SaveInterface" type="Fedex\CartGraphQl\Model\Note\Command\Save"/>
    <preference for="Fedex\CartGraphQl\Api\UpdateOrderDeliveryInterface"
                type="Fedex\CartGraphQl\Model\Resolver\UpdateOrderDelivery\DataHandler" />
    <type name="Fedex\CartGraphQl\Model\Resolver\UpdateOrderDelivery\DataHandler">
        <arguments>
            <argument name="deliveryDataHandlers" xsi:type="array">
                <item name="pickupData" xsi:type="object">Fedex\CartGraphQl\Model\Resolver\UpdateOrderDelivery\Data\PickupData</item>
                <item name="shippingData" xsi:type="object">Fedex\CartGraphQl\Model\Resolver\UpdateOrderDelivery\Data\ShippingData</item>
                <item name="contactData" xsi:type="object">Fedex\CartGraphQl\Model\Resolver\UpdateOrderDelivery\Data\ContactData</item>
            </argument>
        </arguments>
    </type>
    <preference for="Fedex\CartGraphQl\Api\CollectRatesInterface" type="Fedex\CartGraphQl\Model\Address\CollectRates" />
    <type name="Fedex\CartGraphQl\Model\Address\CollectRates">
        <arguments>
            <argument name="collectRates" xsi:type="array">
                <item name="pickupRate" xsi:type="object">Fedex\CartGraphQl\Model\Address\CollectRates\PickupRate</item>
                <item name="shippingRate" xsi:type="object">Fedex\CartGraphQl\Model\Address\CollectRates\ShippingRate</item>
            </argument>
        </arguments>
    </type>
    <preference for="Fedex\CartGraphQl\Api\RecipientsBuilderInterface" type="Fedex\CartGraphQl\Model\RateQuote\RecipientsBuilder"/>
    <type name="Fedex\CartGraphQl\Model\RateQuote\RecipientsBuilder">
        <arguments>
            <argument name="deliveryData" xsi:type="array">
                <item name="pickupData" xsi:type="object">Fedex\CartGraphQl\Model\RateQuote\RecipientsBuilder\PickupData</item>
                <item name="shippingData" xsi:type="object">Fedex\CartGraphQl\Model\RateQuote\RecipientsBuilder\ShippingData</item>
            </argument>
        </arguments>
    </type>
    <preference for="Fedex\CartGraphQl\Api\PlaceOrderRequestInterface" type="Fedex\CartGraphQl\Model\PlaceOrder\RequestData"/>
    <type name="Fedex\CartGraphQl\Model\PlaceOrder\RequestData">
        <arguments>
            <argument name="deliveryData" xsi:type="array">
                <item name="pickupData" xsi:type="object">Fedex\CartGraphQl\Model\PlaceOrder\RequestData\PickupData</item>
            </argument>
        </arguments>
    </type>
</config>
