<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <!-- Schedule Cron every hour to delete customer data file -->
    <group id="fedex_delete_files_crongroup">
        <job name="delete_customer_data_csv_file" instance="Fedex\CustomerExportEmail\Cron\DeleteCustomerDataFile" method="deleteCustomerDataFile">
            <config_path>fedex/cid_psg_configuration_group/delete_authorized_excel_cron</config_path>
        </job>
    </group>
</config>
