<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="fedex_csp" sortOrder="700" showInDefault="1" showInWebsite="1" showInStore="1" translate="label">
            <label>Content Security Policy (CSP)</label>
            <tab>general</tab>
            <resource>Fedex_CSP::config_fedex_csp</resource>
            <group id="csp_whitelist" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" translate="label">
                <label>CSP Whitelist</label>
                <field id="enabled" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Enable CSP Whitelist</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="create_entry" translate="label" sortOrder="2" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Create Entry</label>
                    <frontend_model>Fedex\CSP\Block\Adminhtml\Form\Field\CreateEntry</frontend_model>
                </field>
                <field id="entries_list" translate="label" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Entries List</label>
                    <frontend_model>Fedex\CSP\Block\Adminhtml\Form\Field\EntriesList</frontend_model>
                </field>
            </group>
        </section>
        <section id="environment_toggle_configuration" translate="label">
            <group id="environment_toggle" translate="label">
                <field id="tiger_d196844" translate="label" type="select" sortOrder="720" showInDefault="1" showInWebsite="0" showInStore="0" canRestore="0">
                    <label>Tiger Team - D-196844 JS Error: SecurityPolicyViolation: blockedURI = inline (https://www.office.fedex.com/default/checkout/cart/)</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
