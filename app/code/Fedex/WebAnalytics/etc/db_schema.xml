<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="company" resource="default" engine="innodb" comment="Company Table">
        <column xsi:type="boolean" name="content_square" nullable="false" comment="Enable Content Square"/>
        <column xsi:type="boolean" name="adobe_analytics" nullable="false" comment="Enable Adobe Analytics - GDL"/>
        <column xsi:type="boolean" name="app_dynamics" nullable="false" comment="Enable AppDynamics"/>
        <column xsi:type="boolean" name="forsta" nullable="false" comment="Enable Forsta"/>
        <column xsi:type="boolean" name="nuance" nullable="false" comment="Enable Nuance"/>
    </table>
</schema>
