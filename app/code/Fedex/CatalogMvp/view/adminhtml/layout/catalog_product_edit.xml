<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="Fedex_CatalogMvp::js/custom-date-validation.js"/>
        <script src="Fedex_CatalogMvp::js/configure-popup/main.js"/>

    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Catalog\Block\Adminhtml\Product\Composite\Configure" name="product_edit_timezone_info" template="Fedex_CatalogMvp::admin_timezone.phtml"/>
	    <block class="Fedex\CatalogMvp\Block\Adminhtml\Catalog\Product\ModelPopup" name="catalog-product-popup" template="Fedex_CatalogMvp::catalog/product/modelpopup.phtml">
            <arguments>
                        <argument name="fxo_cm_view_model" xsi:type="object">Fedex\FXOCMConfigurator\ViewModel\FXOCMHelper</argument>
                </arguments>
        </block>
        </referenceContainer>
    </body>
</page>
