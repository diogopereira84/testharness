<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.top">
            <block class="Fedex\CatalogMvp\Block\Adminhtml\Catalog\Product\ModelPopup"
            name="catalog.mvp.product.setting"
            template="Fedex_CatalogMvp::shared-catalog-product-settings-form.phtml" before="-">
                <arguments>
                    <argument name="mvphelper_mvp_setting_popup" xsi:type="object">Fedex\CatalogMvp\ViewModel\MvpHelper</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="catalogmvp_iframe_index_index" template="Fedex_CatalogMvp::customer_admin_configurator.phtml"/>
        </referenceContainer>
    </body>
</page>
