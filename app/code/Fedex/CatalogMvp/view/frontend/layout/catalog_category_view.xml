<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block class="Magento\Framework\View\Element\Template" name="fxo_cm_sdk" template="Fedex_FXOCMConfigurator::sdk.phtml">
                <arguments>
                    <argument name="view_model" xsi:type="object">Fedex\FXOCMConfigurator\ViewModel\FXOCMHelper</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceContainer name="top.container">
            <block class="Magento\Framework\View\Element\Template" name="delete.product.tost.msg" before="add.to.cart.tost.msg" template="Fedex_CatalogMvp::delete_success.phtml">
                <arguments>
                    <argument name="view_model" xsi:type="object">Fedex\CatalogMvp\ViewModel\MvpHelper</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Template" name="preview_block" template="Fedex_CatalogMvp::preview.phtml">
                <arguments>
                     <argument name="fxo_cm_view_model" xsi:type="object">Fedex\FXOCMConfigurator\ViewModel\FXOCMHelper</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
