                    // Function for saving the toggle state
                    function saveToggleState(state) {
                        localStorage.setItem('customizeToggleState', state);
                    }

                    // Fuction for loading and applying the toggle state
                    function loadToggleState() {
                        const savedState = localStorage.getItem('customizeToggleState');
                        if (savedState === 'false') {
                            customizeToggleSwitch.checked = false;
                            customizeContainer.classList.add('off');
                        }
                    }

                    // Event listener for toggle switch change
                    customizeToggleSwitch.addEventListener('change', function() {
                        if (!customizeToggleSwitch.checked) {
                            customizeContainer.classList.add('off');
                            saveToggleState('false');
                        } else {
                            customizeContainer.classList.remove('off');
                            saveToggleState('true');
                        }
                    });

                    // Load and apply toggle state on page load
                    loadToggleState();
