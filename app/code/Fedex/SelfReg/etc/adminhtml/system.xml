<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<section id="selfreg_setting" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
			<label>SelfReg Settings</label>
			<tab>fedex_configurations</tab>
			<resource>Fedex_EnvironmentManager::environmentmanager_config</resource>
			<group id="email_setting" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>Email Settings</label>
                <field id="admin_pending" type="select" translate="label" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Email template Admin Approve Pending</label>
                    <source_model>Fedex\SelfReg\Model\Config\Source\TemplateOptions</source_model>
                </field>
				<field id="user_approve" type="select" translate="label" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Email template User Approve</label>
                    <source_model>Fedex\SelfReg\Model\Config\Source\TemplateOptions</source_model>
                </field>
				<field id="header_image_url" type="text" translate="label" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
					<label>Header Image Url</label>
				</field>
			</group>
			<group id="notification_setting" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>Notification Settings</label>
                <field id="add_item" type="textarea" translate="label" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Add Catalog Item Message</label>
                    <comment>This message will be displayed when a user adds an item to the catalog. If left empty, the default message will be used.</comment>
                </field>
				<field id="move_item" type="textarea" translate="label" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Move Catalog Item Message</label>
                    <comment>This message will be displayed when a user moves an item to a folder. If left empty, the default message will be used.</comment>
                </field>
				<field id="delete_item" type="textarea" translate="label" sortOrder="30" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Delete Catalog Item Message</label>
                    <comment>This message will be displayed when a user deletes an item from the catalog. If left empty, the default message will be used.</comment>
                </field>
			</group>
		</section>
		<section id="environment_toggle_configuration" translate="label" sortOrder="1001" showInDefault="1" showInWebsite="0" showInStore="0">
            <group id="environment_toggle" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <field id="mazegeeks_d226789_quote_link_redirection_from_email" translate="label" type="select" sortOrder="480" showInDefault="1" showInWebsite="0" showInStore="0" canRestore="0">
                    <label>Maze Geeks - D-226789 - Quote Link Redirection from Email</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
		<tab id="wiremock_service" translate="label" sortOrder="10">
            <label>Wiremock Service</label>
		</tab>	
		<section id="wiremock_service" sortOrder="11" showInWebsite="1" showInStore="1" showInDefault="1">
			<label>Wiremock Settings</label>
			<tab>wiremock_service</tab>
			<resource>Fedex_EnvironmentManager::environmentmanager_config</resource>
			<group id="selfreg_wiremock_group" translate="label" type="text" sortOrder="1001" showInStore="1" showInDefault="1" showInWebsite="1">
			   <label>WireMock Settings Configuration</label>
			   <field id="fcl_login_api_wiremock_enable" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0" canRestore="0">
                    <label>FCL Login Wiremock Enabled</label>
		    <comment>Access {host}/selfreg/landing/mock for fcl mock login. If wiremock enabled value is yes.</comment>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
			   <field id="fcl_login_api_wiremock_url" translate="label" type="text" sortOrder="2" showInStore="1" showInDefault="1" showInWebsite="1">
			       <label>Wire Mock URL For FCL Login API</label>
				   <depends>
                        <field id="*/*/fcl_login_api_wiremock_enable">1</field>
                    </depends>
			   </field>
			   <field id="delivery__option_api_wiremock_enable" translate="label" type="select" sortOrder="3" showInDefault="1" showInWebsite="0" showInStore="0" canRestore="0">
                    <label>Delivery Api Wiremock Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
				<field id="delivery_option_api_wiremock_url" translate="label" type="text" sortOrder="4" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Wire Mock URL For Delivery Option API</label>
                    <depends>
                        <field id="*/*/delivery__option_api_wiremock_enable">1</field>
                    </depends>
                </field>
			</group>
		</section>
	</system>
</config>
