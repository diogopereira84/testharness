<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="column_mirakl">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Fedex_MarketplaceAdmin::order/items/column/mirakl.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="order_tab_info">
            <action method="setTemplate">
                <argument name="template" translate="true" xsi:type="string">Fedex_MarketplaceAdmin::order/view/tab/info.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="order_shipping_view">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Fedex_MarketplaceAdmin::order/view/info.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="order_items">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Fedex_MarketplaceAdmin::order/view/items.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="default_order_items_renderer">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Fedex_MarketplaceAdmin::order/view/items/renderer/default.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
