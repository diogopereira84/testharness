<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\ReCaptchaUi\Model\CaptchaTypeResolverInterface" type="Fedex\Recaptcha\Model\CaptchaTypeResolver"/>
    <type name="Fedex\Recaptcha\Model\Validator">
        <arguments>
            <argument name="captchaResponseResolver" xsi:type="object">Fedex\Recaptcha\Model\CaptchaResponseResolver</argument>
        </arguments>
    </type>
    <type name="Magento\ReCaptchaUi\Model\ValidationConfigResolver">
        <arguments>
            <argument name="validationConfigProviders" xsi:type="array">
                <item name="recaptcha_v3" xsi:type="object">Fedex\Recaptcha\Model\Frontend\ValidationConfigProvider\Proxy</item>
            </argument>
        </arguments>
    </type>
</config>
