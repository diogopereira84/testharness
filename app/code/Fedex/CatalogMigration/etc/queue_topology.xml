<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="urn:magento:framework-message-queue:etc/topology.xsd">
    <exchange name="magento.catalogMigration" type="topic" connection="amqp">
        <binding id="magento.catalogMigration.catalogMigration" topic="catalogMigration" destinationType="queue"
    destination="catalogMigration"/>
    </exchange>
    <exchange name="magento.productImgRemove" type="topic" connection="amqp">
        <binding id="magento.productImgRemove.productImgRemove" topic="productImgRemove" destinationType="queue"
    destination="productImgRemove"/>
    </exchange>
</config>
