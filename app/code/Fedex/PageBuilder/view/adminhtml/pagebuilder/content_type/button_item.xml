<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_PageBuilder:etc/content_type.xsd">
    <type name="button-item">
        <appearances>
            <appearance name="default" master_template="Fedex_PageBuilder/content-type/button-item/default/master">
                <elements>
                    <element name="link">
                        <attribute name="aria-label" source="aria-label"/>
                        <attribute name="data-testid" source="data-testid"/>
                        <attribute name="data-analytics" source="data-analytics"/>
                    </element>
                    <element name="empty_link">
                        <attribute name="aria-label" source="aria-label"/>
                        <attribute name="data-testid" source="data-testid"/>
                    </element>
                    <element name="link_text">
                        <attribute name="aria-label" source="aria-label"/>
                        <attribute name="data-testid" source="data-testid"/>
                    </element>
                </elements>
            </appearance>
        </appearances>
    </type>
</config>
