<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_PageBuilder:etc/content_type.xsd">
    <type name="row">
        <appearances>
            <appearance name="contained"
                        master_template="Fedex_PageBuilder/content-type/row/contained/master">
                <elements>
                    <element name="video_overlay">
                        <attribute name="aria-label" source="aria-label"/>
                    </element>
                </elements>
            </appearance>
            <appearance name="full-width"
                        master_template="Fedex_PageBuilder/content-type/row/full-width/master">
                <elements>
                    <element name="video_overlay">
                        <attribute name="aria-label" source="aria-label"/>
                    </element>
                </elements>
            </appearance>
            <appearance name="full-bleed"
                        master_template="Fedex_PageBuilder/content-type/row/full-bleed/master">
                <elements>
                    <element name="video_overlay">
                        <attribute name="aria-label" source="aria-label"/>
                    </element>
                </elements>
            </appearance>
        </appearances>
    </type>
</config>
