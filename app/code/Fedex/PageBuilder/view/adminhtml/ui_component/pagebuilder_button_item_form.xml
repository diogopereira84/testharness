<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd" extends="pagebuilder_base_form">
    <fieldset name="content">
        <field name="data-analytics" sortOrder="80" formElement="input">
            <settings>
                <dataType>text</dataType>
                <validation>
                    <rule name="validate-string" xsi:type="boolean">true</rule>
                </validation>
                <label translate="true">Data Analytics</label>
            </settings>
        </field>
    </fieldset>
    <fieldset name="advanced">
        <field name="aria-label" sortOrder="80" formElement="input">
            <settings>
                <dataType>text</dataType>
                <label translate="true">Aria Label</label>
                <notice translate="true">Aria label description.
                </notice>
            </settings>
        </field>
        <field name="data-testid" sortOrder="80" formElement="input">
            <settings>
                <dataType>text</dataType>
                <label translate="true">Automation Data Test ID</label>
                <notice translate="true">Automation Data Test ID.
                </notice>
            </settings>
        </field>
    </fieldset>
</form>
