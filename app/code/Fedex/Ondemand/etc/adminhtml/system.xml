<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<section id="ondemand_setting" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
			<label>Ondemand Settings</label>
			<tab>fedex_configurations</tab>
			<resource>Fedex_EnvironmentManager::environmentmanager_config</resource>
			<group id="category_setting" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>Category Setup</label>
                <field id="epro_print" type="select" translate="label" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>B2B Print Product Category</label>
                    <source_model>Fedex\Ondemand\Model\Config\Source\CategoryOptions</source_model>
                </field>
                <field id="office_supplies_category" type="select" translate="label" sortOrder="11" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>B2B Office Supplies Category</label>
                    <source_model>Fedex\Ondemand\Model\Config\Source\CategoryOptions</source_model>
                </field>
                <field id="office_supplies_category_label" type="text" translate="label" sortOrder="11" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>B2B Office Supplies Category Label</label>
                    <comment>This value determines how the category appears in the Topmenu header.</comment>
                </field>
                <field id="spm_supplies_category" type="select" translate="label" sortOrder="12" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>B2B Shipping Packing Mailing Supplies Category</label>
                    <source_model>Fedex\Ondemand\Model\Config\Source\CategoryOptions</source_model>
                </field>
                <field id="spm_supplies_category_label" type="text" translate="label" sortOrder="13" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>B2B Shipping Packing Mailing Supplies Category Label</label>
                    <comment>This value determines how the category appears in the Topmenu header.</comment>
                </field>
                <field id="b2b_default_store" type="select" sortOrder="13" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				    <label>B2B Default Store</label>
				    <source_model>Magento\Store\Model\System\Store</source_model>
				</field>
                <field id="default_shared_catalog" type="select" sortOrder="20" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
                    <label>B2B Default Shared Catalog ID</label>
                    <source_model>Magento\SharedCatalog\Model\Source\SharedCatalog</source_model>
                    <comment>The Shared Catalog ID with default print products to be associated with a site when its first created.</comment>
                </field>
			</group>
		</section>
        <section id="environment_toggle_configuration" translate="label">
            <group id="environment_toggle" translate="label">
                <field id="tiger_d239305" translate="label" type="select" sortOrder="1502" showInDefault="1" showInWebsite="0" showInStore="0" canRestore="0">
                    <label>Tiger - D-239305 INCIDENT #INC032681281: Production Defect: Unable to view Essendant product listing pages and assign Essendant products to Commercial customer shared catalogs</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
	</system>
</config>
