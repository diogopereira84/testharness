<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
        <section id="fedex" translate="label" sortOrder="1001" showInDefault="1" showInWebsite="0" showInStore="0">
            <resource>Fedex_EnvironmentManager::environmentmanager_config</resource>
            <group id="marketplace_configuration" translate="label" sortOrder="1500" showInDefault="1" showInWebsite="1" showInStore="1">
                <field id="marketplace_downtime_message_title" translate="label" type="text" sortOrder="7" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>3p Site Downtime Message Title</label>
                    <comment>For Marketplace Downtime Notification</comment>
                </field>
                <field id="marketplace_downtime_message_body" translate="label comment" type="text" sortOrder="8" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>3p Site Downtime Message Body</label>
                    <comment>For Marketplace Downtime Notification</comment>
                </field>
                <group id="mock_punchout_payload" translate="label" type="text" sortOrder="999" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Mock for Punchout Payload</label>
                    <field id="mock_payload_enabled" type="select" translate="label" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                        <label>Enable Mock for Punchout Payload</label>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                        <config_path>fedex/marketplace_configuration/mock_payload_enabled</config_path>
                    </field>
                    <field id="mock_payload_skus" type="text" translate="label" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                        <label>List of Products SKU to Mock</label>
                        <comment>List all product SKUs separated by commas. For example: sku1,sku2,sku3</comment>
                        <config_path>fedex/marketplace_configuration/mock_payload_skus</config_path>
                    </field>
                    <field id="mock_payload" type="textarea" translate="label" sortOrder="30" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                        <label>Enable Mock for Punchout Payload</label>
                        <config_path>fedex/marketplace_configuration/mock_payload</config_path>
                    </field>
                </group>
            </group>
            <group id="navink_configuration" translate="label comment" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>NavInk Configurations</label>
                <field id="from_id" translate="label comment" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Reseller ID</label>
                    <comment>This is the "From" field on the xml.</comment>
                </field>
                <field id="to_id" translate="label comment" type="text" sortOrder="2" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Default Customer Acct</label>
                    <comment>This is the "To" field on the xml.</comment>
                </field>
                <field id="sender_id" translate="label comment" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Sender Identity</label>
                </field>
                <field id="sender_shared_secret" translate="label" type="obscure" sortOrder="4" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Navink Secret Key</label>
                    <backend_model>Magento\Config\Model\Config\Backend\Encrypted</backend_model>
                    <attribute type="shared">1</attribute>
                    <comment>This field will store the Navink Secret Key securely and encrypted</comment>
                </field>
                <field id="account_number" translate="label comment" type="text" sortOrder="5" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Account Number</label>
                </field>
                <field id="navitor_url" translate="label comment" type="text" sortOrder="6" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Url</label>
                </field>
                <field id="navitor_auth_url" translate="label comment" type="text" sortOrder="9" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Authorization Url</label>
                </field>
                <field id="navitor_product_info_url" translate="label comment" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Product Info Url</label>
                </field>
                <field id="navitor_reorder_url" translate="label comment" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Reorder Url</label>
                </field>
            </group>
        </section>
        <section id="environment_toggle_configuration">
            <group id="environment_toggle">
                <field id="tiger_e426628_shops_connection_data_change" translate="label comment" type="select" sortOrder="430" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Tiger Team - E-426628 - B-2088298 Shops Connection Data Change from Stores</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="tiger_d217530_shopping_cart_not_clearing_3p_items" translate="label comment" type="select" sortOrder="450" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Tiger Team - D-217530 Shopping cart is not clearing 3p items when a mixed cart.</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="tiger_d220695_cbb_punchout_heartbeat_url" translate="label comment" type="select" sortOrder="450" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Tiger Team - D-220695 CBB punchout heartbeat dynamic url.</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="hawks_d229949_customer_section_load_marketplace_evict_unnecessary_calls" translate="label" type="select" sortOrder="999" showInDefault="1" showInWebsite="0" showInStore="0" canRestore="0">
                    <label>Hawks D-229949 - Customer Section Marketplace evict unnecessary calls</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="tiger_tk4611007_punchout_seller_id" translate="label comment" type="select" sortOrder="500" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Tiger Team - TK-4611007 Add Seller ID in Marketplace Punchout XML</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
	</system>
</config>
