<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="fedex">
            <group id="general">
                <field id="api_timeout" translate="label" type="text" sortOrder="6" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>API HTTP timeout</label>
                    <comment>Timeout for HTTP Post request within X seconds.</comment>
                    <validate>required-entry validate-zero-or-greater validate-number</validate>
                </field>
            </group>
        </section>
    </system>
</config>

