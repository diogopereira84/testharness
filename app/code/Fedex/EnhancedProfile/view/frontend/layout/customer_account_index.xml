<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <move element="sidebar.main" destination="customer.left.nav" before="-" />
        <move element="customer.left.nav" destination="columns" after="customer_account_dashboard_info_top" />
        <referenceContainer name="columns">
            <container name="customer.left.nav" htmlTag="div" htmlClass="sidebar sidebar-main" after="main">
            </container>
            <block class="Magento\Framework\View\Element\Template" name="contact_info" template="Fedex_EnhancedProfile::contact_info.phtml"/>
        </referenceContainer>
    </body>
</page>
