<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="checkout.root">
            <arguments>
                <argument name="jsLayout" xsi:type="array">
                    <item name="components" xsi:type="array">
                        <item name="checkout" xsi:type="array">
                                <item name="children" xsi:type="array">
                                    <item name="sidebar" xsi:type="array">
                                        <item name="children" xsi:type="array">
                                            <item name="promo-account-container" xsi:type="array">
                                                <item name="component" xsi:type="string">Fedex_Cart/js/view/summary/promo_account/promo-account-container</item>
                                                <item name="displayArea" xsi:type="string">summary</item>
                                                <item name="sortOrder" xsi:type="string">120</item>
                                                <item name="children" xsi:type="array">
                                                    <item name="promo-account-warning-message" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_Cart/js/view/summary/promo_account/promo-account-warning-message</item>
                                                        <item name="displayArea" xsi:type="string">summary</item>
                                                    </item>
                                                    <item name="discount-messages" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_Cart/js/view/summary/promo_account/discount-messages</item>
                                                        <item name="displayArea" xsi:type="string">summary</item>
                                                    </item>
                                                    <item name="promo-code" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_Cart/js/view/summary/promo_account/promo-coupon/promo-coupon</item>
                                                        <item name="displayArea" xsi:type="string">summary</item>
                                                    </item>
                                                    <item name="fedex-account-discount" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_Cart/js/view/summary/promo_account/fedex-account-discount/fedex-account-discount</item>
                                                        <item name="displayArea" xsi:type="string">summary</item>
                                                    </item>
                                                    <item name="agreementss" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_SubmitOrderSidebar/js/view/checkout-agreements</item>
                                                        <item name="sortOrder" xsi:type="string">101</item>
                                                        <item name="displayArea" xsi:type="string">after-place-agreements</item>
                                                        <item name="dataScope" xsi:type="string">checkoutAgreements</item>
                                                        <item name="provider" xsi:type="string">checkoutProvider</item>
                                                    </item>
                                                    <item name="review-submit-order" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_Cart/js/view/summary/promo_account/review-order-submit-button</item>
                                                        <item name="displayArea" xsi:type="string">summary</item>
                                                    </item>
                                                    <item name="modal-time-mismatch" xsi:type="array">
                                                        <item name="component" xsi:type="string">Fedex_SubmitOrderSidebar/js/view/modal-time-mismatch</item>
                                                        <item name="displayArea" xsi:type="string">summary</item>
                                                    </item>
                                                </item>
                                            </item>
                                        </item>
                                    </item>
                                </item>
                            </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
