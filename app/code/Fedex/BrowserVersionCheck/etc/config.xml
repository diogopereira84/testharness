<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <web>
            <browser_version_check>
                <enable>0</enable>
                <heading>Browser Update Required</heading>
                <subheading>Please upgrade for better experience</subheading>
                <chrome_minimum_version>128.0.6613.137</chrome_minimum_version>
                <edge_minimum_version>128.0.2739.67</edge_minimum_version>
                <firefox_minimum_version>126.0.1</firefox_minimum_version>
                <safari_minimum_version>17.5</safari_minimum_version>
            </browser_version_check>
        </web>
    </default>
</config>
