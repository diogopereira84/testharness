<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="after.body.start">
            <block class="Magento\Framework\View\Element\Template"
                   name="fedex.browser.version.check"
                   template="Fedex_BrowserVersionCheck::browser/version_check.phtml">
                <arguments>
                    <argument name="viewModel" xsi:type="object">Fedex\BrowserVersionCheck\ViewModel\Config</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
